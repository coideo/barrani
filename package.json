{
  "name": "@coideo/barrani",
  "description": "tailwind + react-query + react-hook-form",
  "version": "0.1.2",
  "main": "./build/index.js",
  "module": "./build/index.es.js",
  "types": "./build/index.d.ts",
  "license": "MIT",
  "files": [
    "build",
    "postcss.config.js",
    "tailwind.config.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "resolutions": {
    "postcss": "8.2.1"
  },
  "scripts": {
    "build-storybook": "build-storybook -s .storybook/public",
    "build": "rollup -c",
    "dev": "start-storybook -p 5000 -s .storybook/public",
    "format": "prettier --write \"**/*.+(ts|tsx|js|json|css|md|mdx|html)\" --loglevel warn",
    "lint": "eslint . --ext .ts,.tsx,.js --cache --fix",
    "test": "jest --coverage",
    "type-check": "tsc --pretty --noEmit",
    "validate": "npm-run-all lint type-check test"
  },
  "dependencies": {
    "@headlessui/react": "^1.0.0",
    "@hookform/error-message": "^2.0.0",
    "@reach/combobox": "^0.15.0",
    "@reach/listbox": "^0.15.0",
    "react-hook-form": "^7.1.1",
    "react-query": "^3.13.9"
  },
  "peerDependencies": {
    "postcss": "^8.2.10",
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@popperjs/core": "^2.9.2",
    "@storybook/addon-a11y": "^6.2.8",
    "@storybook/addon-actions": "^6.2.8",
    "@storybook/addon-links": "^6.2.8",
    "@storybook/addon-storysource": "^6.2.8",
    "@storybook/addon-viewport": "^6.2.8",
    "@storybook/addons": "^6.2.8",
    "@storybook/react": "^6.2.8",
    "@storybook/theming": "^6.2.8",
    "@tailwindcss/forms": "^0.3.2",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/react-hooks": "^5.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.41",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-loader": "^8.2.2",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "msw": "^0.28.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.10",
    "postcss-cli": "^8.3.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.2",
    "rollup": "^2.45.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "tailwindcss": "^2.1.1",
    "ts-jest": "^26.5.5",
    "ts-loader": "^8.1.0",
    "typescript": "^4.2.4",
    "whatwg-fetch": "^3.6.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm test"
    }
  },
  "lint-staged": {
    "*.*(ts|tsx|js)": "eslint . --ext .ts,.tsx,.js --cache --fix"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
