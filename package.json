{
  "name": "@coideo/barrani",
  "description": "tailwind + react-query + react-hook-form",
  "version": "0.0.10",
  "main": "./build/index.js",
  "module": "./build/index.es.js",
  "types": "./build/index.d.ts",
  "license": "MIT",
  "files": [
    "build",
    "postcss.config.js",
    "tailwind.config.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build-storybook-static": "build-storybook -s .storybook/public",
    "build-storybook": "npm-run-all build-tailwind build-storybook-static",
    "build-tailwind": "postcss src/styles/index.css -o src/styles/tailwind.css",
    "build": "rollup -c",
    "dev": "npm-run-all build-tailwind start-storybook",
    "format": "prettier --write \"**/*.+(ts|tsx|js|json|css|md|mdx|html)\" --loglevel warn",
    "lint": "eslint . --ext .ts,.tsx,.js --cache --fix",
    "start-storybook": "start-storybook -p 5000 -s .storybook/public",
    "test": "jest --coverage",
    "type-check": "tsc --pretty --noEmit",
    "validate": "npm-run-all lint type-check test"
  },
  "dependencies": {
    "@headlessui/react": "^0.2.0",
    "@hookform/error-message": "^0.0.5",
    "@reach/combobox": "^0.12.1",
    "@reach/listbox": "^0.12.1",
    "react-hook-form": "^6.12.2",
    "react-query": "^2.26.4"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-react": "^7.12.10",
    "@popperjs/core": "^2.5.4",
    "@storybook/addon-a11y": "^6.1.10",
    "@storybook/addon-actions": "^6.1.10",
    "@storybook/addon-links": "^6.1.10",
    "@storybook/addon-storysource": "^6.1.10",
    "@storybook/addon-viewport": "^6.1.10",
    "@storybook/addons": "^6.1.10",
    "@storybook/react": "^6.1.10",
    "@storybook/theming": "^6.1.10",
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/jest": "^26.0.18",
    "@types/node": "^14.14.12",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/testing-library__jest-dom": "^5.9.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^4.9.1",
    "@typescript-eslint/parser": "^4.9.1",
    "autoprefixer": "^9",
    "babel-loader": "^8.2.2",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-standard": "^4.1.0",
    "fork-ts-checker-webpack-plugin": "^6.0.5",
    "husky": "^4.3.5",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.3",
    "msw": "^0.24.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^7",
    "postcss-cli": "^8.3.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-test-renderer": "^17.0.1",
    "rollup": "^2.34.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.29.0",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "whatwg-fetch": "^3.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm test"
    }
  },
  "lint-staged": {
    "*.*(ts|tsx|js)": "eslint . --ext .ts,.tsx,.js --cache --fix"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  }
}
